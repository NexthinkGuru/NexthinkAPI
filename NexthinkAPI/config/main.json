        "NQL_Export": {
            "Method": "POST",
            "Path": "/v1/nql/export"
        },
        "NQL_Export_Status": {
            "Method": "GET",
            "Path": "/v1/nql/status/"
        },
        "WF_Exec": {
            "Method": "POST",
            "Path": "/v2/workflows/execute"
        },
        "WF_List": {
            "Method": "GET",
            "Path": "/v1/workflows",
            "Details": "/details?nql-id="
        }
