    "APIs": {
        "BASE": "/api",
        "OAuth": {
            "Method": "POST",
            "Path": "/oauth2/default/v1/token"
        },
        "Campaign": {
            "Method": "POST",
            "Path": "/v1/euf/campaign/trigger"
        },
        "RA_Exec": {
            "Method": "POST",
            "Path": "/v1/act/execute"
        },
        "RA_List": {
            "Method": "GET",
            "Path": "/v1/act/remote-action",
            "Details": "/details?nql-id="
