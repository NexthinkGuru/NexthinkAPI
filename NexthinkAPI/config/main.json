                "Code": 200,
                "Status": "OK",
                "Message": "Successful triggering"
            },
            {
                "Code": 400,
                "Status": "Bad request",
                "Message": "Invalid enrichment, see error codes for details",
                "keys": "code,message"
            },            
            {
                "Code": 401,
                "Status": "Unauthorized",
                "Message": "No valid authentication credentials.",
                "keys": "code,message"
            }
        ],
        "Enrich": [
            {
                "Code": 207,
                "Status": "Partial success enrichment response",
                "Message": "Some invalid enrichment, see error codes for details",
                "keys": "status,errors"
            },
            {
                "Code": 400,
                "Status": "Bad request",
                "Message": "Invalid enrichment, see error codes for details",
                "keys": "status,errors"
            },            
            {
                "Code": 401,
                "Status": "Unauthorized",
                "Message": "No valid authentication credentials."
            },            
            {
                "Code": 403,
                "Status": "No Permission",
                "Message": "No permission to trigger enrichment",
                "keys": "message"
            }
        ],
        "RA_List": [
            {
                "Code": 400,
                "Status": "Bad request",
                "Message": "Invalid request, see error code for details"
            },   
            {
                "Code": 401,
                "Status": "Unauthorized",
                "Message": "No valid authentication credentials."
            },
            {
                "Code": 403,
                "Status": "No Permission",
                "Message": "No permission to get remote action(s)",
                "keys": "message"
            }
        ],
        "RA": [
            {
                "Code": 400,
                "Status": "Bad request",
                "Message": "Invalid request, see error code for details"
            },   
            {
                "Code": 401,
                "Status": "Unauthorized",
                "Message": "No valid authentication credentials."
            },
            {
                "Code": 403,
                "Status": "No Permission",
                "Message": "No permission to execute remote action",
                "keys": "message"
            }
        ],
        "NQL": [
            {
                "Code": 400,
                "Status": "Bad request",
                "Message": "Invalid request, see the error code for details"
            },            
            {
                "Code": 403,
                "Status": "Unauthorized",
                "Message": "No permission to execute queries",
                "keys": "message,code,source"
            },
            {
                "Code": 404,
                "Status": "Query not found",
                "Message": "Given queryId doesn't exist"
            },
            {
                "Code": 500,
                "Status": "Internal server error",
                "Message": "Internal server error",
