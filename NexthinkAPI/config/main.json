            },
            {
                "Code": 503,
                "Status": "Internal error",
                "Message": "Internal error"
            }
        ],
        "WF": [
            {
                "Code": 400,
                "Status": "Bad request",
                "Message": "Invalid request, see error code for details"
            },   
            {
                "Code": 401,
                "Status": "Unauthorized",
                "Message": "No valid authentication credentials."
            },   
            {
                "Code": 403,
                "Status": "No Permission",
                "Message": "No permission to execute workflow"
            }
        ]
    },

    "APIs": {
        "BASE": "/api/v1",
        "DETAILS": "/details?nql-id=",
        "OAUTH": {
            "Method": "POST",
            "uri": "/token"
        },
        "Campaign": {
            "Method": "POST",
            "uri": "/euf/campaign/trigger"
        },
        "RA_Exec": {
            "Method": "POST",
            "uri": "/act/execute"
        },
        "RA_List": {
            "Method": "GET",
            "uri": "/act/remote-action"
        },
        "Enrich": {
            "Method": "POST",
            "uri": "/enrichment/data/fields",
            "Headers" : {
                "x-enrichment-trace-id": "([guid]::NewGuid()).Guid"
            }
        },
        "NQL": {
            "Method": "POST",
            "uri": "/execute"
        },
        "WF_Exec": {
            "Method": "POST",
            "uri": "/workflow/execute"
        },
        "WF_List": {
            "Method": "GET",
            "uri": "/workflow/workflow"
        }
    },

    "EnrichmentIDMap": {
        "device.name": "device/device/name",
        "device.uid": "device/device/uid",
        "user.sid": "user/user/sid",
        "user.uid": "user/user/uid",
        "binary.uid": "binary/binary/uid",
        "package.uid": "package/package/uid"
    }

}

