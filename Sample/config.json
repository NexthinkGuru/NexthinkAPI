{
    "_Info": "This is the JSON based configuration for the Nexthink Infinity API's",
    "Logging": {
        "LogRetentionDays": 21,
        "LogLevel": "DEBUG",
        "Path": "./Logs/"
    },
    "NexthinkAPI": {
        "InstanceName": "naps",
        "Region": "us",
        "OAuthCredentialEntry": "naps-oauth",
        "RequestBatchSize": 1000
    },
    "Proxy": {
        "UseSystemProxy": true,
        "UseDefaultCredentials": false
    }
}